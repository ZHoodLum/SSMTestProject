<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neusoft.mapper.DeptMapper">

	<select id="getById" resultType="com.neusoft.entity.Dept">
		select * from tbldept where deptid = #{deptid}
	</select>
	
	<select id="getAll" resultType="com.neusoft.entity.Dept">
		select * from tbldept 
	</select>
</mapper>